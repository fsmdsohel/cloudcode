{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsc-watch --onSuccess \"bash -c 'tsc-alias --resolve-full-paths && node dist/server.js'\"",
